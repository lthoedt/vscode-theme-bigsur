	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "systemGray4_Dark"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"meta.method-call",
				"variable.parameter"
				// "meta.method-call",
			],
			"settings": {
				"foreground": "fontColorPrimary_Dark",
				"fontStyle": ""
			}
		},
		{
			"name": "Colors",
			"scope": ["constant.other.color"],
			"settings": {
				"foreground": "fontColorPrimary_Dark"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "systemRed_Dark"
			}
		},
